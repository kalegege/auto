<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
    <classPathEntry location="/Users/kale/utils/mysql-connector-java-5.1.6.jar"/>

    <context id="default" >

    <!--关闭注解信息 -->
    <commentGenerator>
        <property name="suppressAllComments" value="true"/>
    </commentGenerator>

    <jdbcConnection driverClass="com.mysql.jdbc.Driver" connectionURL="jdbc:mysql://125.210.141.30/galaxy" userId="root" password="Hello1234">
    </jdbcConnection>

    <javaTypeResolver>
        <property name="forceBigDecimals" value="false"/>
    </javaTypeResolver>

    <!-- model-->
    <javaModelGenerator targetPackage="com.wasu.auto.model" targetProject=".\src\main\java">
        <property name="enableSubPackages" value="true"/>
        <property name="trimStrings" value="true"/>
    </javaModelGenerator>

    <!-- xml-->
    <sqlMapGenerator targetPackage="com.wasu.auto.dao" targetProject=".\src\main\resources">
        <property name="enableSubPackages" value="true"/>
    </sqlMapGenerator>

    <!-- mapper-->
    <javaClientGenerator type="XMLMAPPER" targetPackage="com.wasu.auto.dao" targetProject=".\src\main\java">
        <property name="enableSubPackages" value="true"/>
    </javaClientGenerator>

    <table tableName="chanel_item" domainObjectName="item"></table>
    </context>
</generatorConfiguration>